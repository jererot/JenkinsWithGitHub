        <th>
            @Html.DisplayName("Curso")
        </th>
        <th>
            @Html.DisplayName("Docente")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CURSO.nom_cur)
        </td>
